    "license": "MIT",
    "scripts": {
        "build": "nest build",
        "dev": "nest start --watch",
        "format": "prettier --write \"**/*.{ts,js}\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "typeorm": "npm run build && npx typeorm -d ./dist/data/data-source.js",
