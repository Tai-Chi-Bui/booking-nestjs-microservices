        "@types/jest": "29.5.14",
        "@types/supertest": "6.0.2",
        "eslint": "9.17.0",
        "globals": "15.14.0",
        "jest": "29.7.0",
        "jest-serial-runner": "1.2.1",
        "prettier": "3.4.2",
        "supertest": "7.0.0",
        "ts-jest": "29.2.5",
        "typemoq": "2.1.0",
        "typescript-eslint": "8.18.2"
    }
